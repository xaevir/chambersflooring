div#myCarousel.carousel.slide
  div.carousel-inner
    div.item.active
      img(src='/img/slide-before.jpg')
      div.container
        div.carousel-caption
          h1.super
            span.highlight-green
              | The <b class="curly-quote">&#8220;</b>Before<b 
              | class="curly-quote">&#8221;</b> Scene at <br /> 
            span.highlight-green Immaculata College
          p.lead Our work in progress as we installed the gymnasium floor
    div.item
      img(src='/img/slide-after.jpg')
      div.container
        div.carousel-caption
          h1.super
            span.highlight-green
              | The <b class="curly-quote">&#8220;</b>After<b 
              | class="curly-quote">&#8221;</b> Scene at <br />
            span.highlight-green Immaculata College
          p.lead Our finished product shining with the fresh coats of finish.
  a.left.carousel-control(href="#myCarousel", data-slide="prev") &lsaquo;
  a.right.carousel-control(href="#myCarousel", data-slide="next") &rsaquo;

